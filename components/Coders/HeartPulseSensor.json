{
    "name": "HeartPulseSensor",
    "supportedControllers": [
        "ATMega328P",
        "ArduinoGemma",
        "ArduinoLeonardo",
        "ArduinoMega",
        "ArduinoMicro",
        "ArduinoNano",
        "ArduinoProMini3v3",
        "ArduinoProMini5v",
        "ArduinoUno",
        "ESP32DevKitC",
        "NodeMCU",
        "RPI3B"
    ],
    "code": {
        "constructors": "    {{classname}} {{varname}};\n",
        "setup": "    {{varname}}.begin({{SIG}});\n",
        "snippetCode": "    //Measure Heart Rate\n    int {{varname}}BPM = {{varname}}.BPM;\n    Serial.println({{varname}}BPM);\n    if ({{varname}}.QS == true) {\n        Serial.println(\"PULSE\");\n        {{varname}}.QS = false;\n    }"
    },
    "license": "MIT",
    "instantiate": {
        "classname": "PulseSensor",
        "ports": [
            "SIG"
        ],
        "varname": "heartpulse"
    },
    "files": [
        "pulse-sensor-arduino.h",
        "pulse-sensor-arduino_license.txt",
        "pulse-sensor-arduino.cpp"
    ],
    "includeHeaders": [
        "pulse-sensor-arduino.h"
    ]
}